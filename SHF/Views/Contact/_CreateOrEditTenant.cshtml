    legend {
        display: block;
        width: inherit !important;
        padding: 0;
        margin-bottom: 5px !important;
        font-size: 20px;
        line-height: inherit;
        color: #333;
        border: 0;
        border-bottom: 0px solid #e5e5e5 !important;
